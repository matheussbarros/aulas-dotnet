
@{
    ViewData["Title"] = "Listar";
}

<h1>Lista de Usuários</h1>

<table class="table">
    <tr>
        <th>Nome</th>
        <th>Data</th>
        <th>E-mail</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nome</td>
            <td>@item.DataNascimento</td>
            <td>@item.Email</td>
        </tr>
    }

</table>

