
@{
    ViewData["Title"] = "Cadastrar";
}

<h2>Cadastrar</h2>
<p>Nome do Usuario: @ViewData["nome"]</p>
<p>CPF: @if(ViewBag.cliente != null){
    @View.cliente.Cpf} </p>
<p>@TempData["msg"]</p>

    <form class="form-group" action="/Cliente/Cadastrar" method="post">
        <div class="form-group">
            <label>Nome</label>
            <input class="form-control" type="text" name="nome" value="" />
        </div>
        <div class="form-group">
            <label>e-mail</label>
            <input class="form-control" type="email" name="email" value="" />
        </div>
        <div class="form-group">
            <label>CPF</label>
            <input class="form-control" type="text" name="cpf" value="" />
        </div>
        <div class="checkbox">
            <label>
                <input type="checkbox" name="maior">
                Maior de Idade
            </label>
        </div>
        <div class="form-group">
            <button class="btn btn-primary" type="submit">Cadastrar</button>
        </div>






    </form>

